			$filter =  array();			
			$kemarin = whereTofilter('tanggal', date("Y-m-d", time() - 86400) , 'date' , 'lt');
			$kemarinlusa = whereTofilter('tanggal', date("Y-m-d", time() - 172800) , 'date' , 'gt');
